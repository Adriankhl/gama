<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="gama.display">
      <implementation
            class="gama.core.display.java2d.Java2DDisplaySurface"
            keyword="java2D">
      </implementation>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="gama.gui.category.gama"
            class="gama.ui.displays.java2d.AWTDisplayView"
            icon="platform:/plugin/gama.ui.base/icons/view.display2.png"
            id="gama.core.application.view.LayeredDisplayView"
            name="Display"
            restorable="false">
      </view>
   </extension>

</plugin>
