<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="gaml"
    nsURI="http://www.gama.msi/lang/gaml/Gaml" nsPrefix="gaml">
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="imports" upperBound="-1"
        eType="#//Import" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="statements" upperBound="-1"
        eType="#//Statement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Import">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="importURI" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Statement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="key" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" eType="#//GamlFacetRef"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="expr" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="facets" upperBound="-1"
        eType="#//FacetExpr" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="block" eType="#//Block"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="else" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Definition" eSuperTypes="#//Statement #//GamlVarRef"/>
  <eClassifiers xsi:type="ecore:EClass" name="FacetRef">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ref" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GamlFacetRef" eSuperTypes="#//FacetRef"/>
  <eClassifiers xsi:type="ecore:EClass" name="FunctionGamlFacetRef" eSuperTypes="#//FacetRef"/>
  <eClassifiers xsi:type="ecore:EClass" name="FacetExpr">
    <eStructuralFeatures xsi:type="ecore:EReference" name="key" eType="#//FacetRef"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="expr" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DefinitionFacetExpr" eSuperTypes="#//FacetExpr #//GamlVarRef"/>
  <eClassifiers xsi:type="ecore:EClass" name="NameFacetExpr" eSuperTypes="#//DefinitionFacetExpr"/>
  <eClassifiers xsi:type="ecore:EClass" name="ReturnsFacetExpr" eSuperTypes="#//DefinitionFacetExpr"/>
  <eClassifiers xsi:type="ecore:EClass" name="ActionFacetExpr" eSuperTypes="#//DefinitionFacetExpr"/>
  <eClassifiers xsi:type="ecore:EClass" name="FunctionFacetExpr" eSuperTypes="#//FacetExpr"/>
  <eClassifiers xsi:type="ecore:EClass" name="Block">
    <eStructuralFeatures xsi:type="ecore:EReference" name="statements" upperBound="-1"
        eType="#//Statement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//Expression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="op" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PairExpr" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractRef" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="Function" eSuperTypes="#//AbstractRef">
    <eStructuralFeatures xsi:type="ecore:EReference" name="args" upperBound="-1" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableRef" eSuperTypes="#//AbstractRef">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" eType="#//GamlVarRef"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GamlVarRef">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TerminalExpression" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TernExp" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ifFalse" eType="#//Expression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ArgPairExpr" eSuperTypes="#//PairExpr">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="arg" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GamlBinaryExpr" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="GamlUnitExpr" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="GamlUnaryExpr" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="MemberRef" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="Array" eSuperTypes="#//Expression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="exprs" upperBound="-1"
        eType="#//Expression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Point" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="UnitName" eSuperTypes="#//Expression"/>
  <eClassifiers xsi:type="ecore:EClass" name="IntLiteral" eSuperTypes="#//TerminalExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="DoubleLiteral" eSuperTypes="#//TerminalExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="ColorLiteral" eSuperTypes="#//TerminalExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="StringLiteral" eSuperTypes="#//TerminalExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="BooleanLiteral" eSuperTypes="#//TerminalExpression"/>
</ecore:EPackage>
