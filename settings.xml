<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<servers>
		<server>
			<id>p2Repo</id>
			<username>${env.USER_ID}</username>
			<password>${env.USER_PWD}</password>
			<filePermissions>666</filePermissions>
			<directoryPermissions>777</directoryPermissions>			
			<configuration>
				<httpConfiguration>
				  <all>
					<connectionTimeout>1200000</connectionTimeout>
					<readTimeout>1200000</readTimeout>
				  </all>
				</httpConfiguration>
		  </configuration>
		</server>
	</servers>

</settings>
