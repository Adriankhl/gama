			assert var0 = [1::2, 2::4, 3::6, 4::8, 5::10, 6::12, 7::14, 8::16]; 
			map<int,int> var1 <- [1::2,3::4,5::6] as_map (each::(each * 2)); 	// var1 equals [2::4, 4::8, 6::12] 
			assert var1 = [2::4, 4::8, 6::12] ; 

		}
	
