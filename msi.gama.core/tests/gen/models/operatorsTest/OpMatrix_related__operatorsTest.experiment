			assert var20 = ['a'::1,'b'::2]; 
			map var21 <- ['a'::1,'b'::2] - ['b'::2]; 	// var21 equals ['a'::1]
			assert var21 = ['a'::1]; 
			map var22 <- ['a'::1,'b'::2] - ['b'::2,'c'::3]; 	// var22 equals ['a'::1]
			assert var22 = ['a'::1]; 
			int var23 <- - (-56); 	// var23 equals 56
			assert var23 = 56; 
			point var24 <- -{3.0,5.0}; 	// var24 equals {-3.0,-5.0}
			assert var24 = {-3.0,-5.0}; 
