			matrix var17 <- 3.5 + matrix([[2,5],[3,4]]); 	// var17 equals matrix([[5.5,8.5],[6.5,7.5]])
			assert var17 = matrix([[5.5,8.5],[6.5,7.5]]); 
			list<int> var18 <- [1,2,3,4,5,6] + [2,4,9]; 	// var18 equals [1,2,3,4,5,6,2,4,9]
			assert var18 = [1,2,3,4,5,6,2,4,9]; 
