			assert var2 = matrix([[1.5,-1.5],[0.5,-0.5]]); 
			list<int> var3 <- [1,2,3,4,5,6] - [2,4,9]; 	// var3 equals [1,3,5,6]
			assert var3 = [1,3,5,6]; 
