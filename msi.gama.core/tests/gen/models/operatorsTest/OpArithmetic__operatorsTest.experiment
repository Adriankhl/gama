			assert var6 = 8.0; 
			float var7 <- 8^1.0; 	// var7 equals 8.0
			assert var7 = 8.0; 
			float var8 <- 8.0^1.0; 	// var8 equals 8.0
			assert var8 = 8.0; 
			float var9 <- 2^0.5; 	// var9 equals sqrt(2)
			assert var9 = sqrt(2); 
			float var10 <- 16.81^0.5; 	// var10 equals sqrt(16.81)
			assert var10 = sqrt(16.81); 
			float var11 <- (10^(-9) = 0); 	// var11 equals false
			assert var11 = false; 
			float var12 <- 4.84 ^ 0.5; 	// var12 equals 2.2
			assert var12 = 2.2; 

		}
	
		test roundOp {
			int var0 <- round (0.51); 	// var0 equals 1
			assert var0 = 1; 
			int var1 <- round (100.2); 	// var1 equals 100
			assert var1 = 100; 
