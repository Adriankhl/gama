		test absOp {
			float var0 <- abs (200 * -1 + 0.5); 	// var0 equals 199.5
			assert var0 = 199.5; 
			int var1 <- abs (-10); 	// var1 equals 10
			assert var1 = 10; 
			int var2 <- abs (10); 	// var2 equals 10
			assert var2 = 10; 
			int var3 <- abs (-0); 	// var3 equals 0
			assert var3 = 0; 

		}
	
